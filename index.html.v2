<!DOCTYPE html>
<html>
<head>
    <title>My Website</title>
    <style>
        /* Set background image */
        body {
            background-image: url('https://images.unsplash.com/photo-1506102383123-c8ef1e872756?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxleHBsb3JlLWZlZWR8Nnx8fGVufDB8fHx8&auto=format&fit=crop&w=800&q=60');
            background-size: cover;
            background-position: center;
        }

        /* Center align the options */
        .options {
            display: flex;
            justify-content: center;
            align-items: center;
            height: 100vh;
        }

        /* Style the buttons */
        .options button {
            margin: 0 10px;
            padding: 10px 20px;
            font-size: 18px;
            background-color: #333;
            color: #fff;
            border: none;
            cursor: pointer;
        }

        /* Style the buttons on hover */
        .options button:hover {
            background-color: #666;
        }
    </style>
</head>
<body>
    <div class="options">
        <button onclick="goToMenu()">MENU</button>
        <button onclick="goToBranch()">BRANCH</button>
    </div>

    <script>
        function goToMenu() {
            // Redirect to menu page
            window.location.href = "menu.html";
        }

        function goToBranch() {
            // Redirect to branch page
            window.location.href = "branch.html";
        }
    </script>
</body>
</html>
